<!DOCTYPE html>
<html lang="${request.locale_name}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="pyramid web application">
    <meta name="author" content="Steve Spicklemire">
    <link rel="shortcut icon" href="${request.static_url('assessdb:static/pyramid-16x16.png')}">

    <title>PHYS/ESCI Assessment Site</title>

    <!-- Bootstrap core CSS -->
    <link href="//oss.maxcdn.com/libs/twitter-bootstrap/3.0.3/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="${request.static_url('assessdb:static/theme.css')}" rel="stylesheet">
    <link rel="shortcut icon" href="${request.static_url('assessdb:static/favicon.ico')}" />
    
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="//oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="//oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>

    <div class="starter-template">
      <div class="container">
        <div class="row">
         <img class="logo img-responsive" src="${request.static_url('assessdb:static/LogowithTagline.png')}" alt="pyramid web framework">
        </div>
        <div class="row">
          <div class="col-md-11">
            <div class="content">
              <h1><span class="smaller">Physics and Earth Space Sciences Intro Assessments</span></h1>
              <table><tr>
              <th width="80"> Term </th> 
              <th width="80"> Subject </th> 
              <th width="80"> Num </th> 
              <th width="80"> Sect </th> 
              <th width="80"> CRN </th> </tr>
              <tal:blk tal:repeat="course courses">
                  <tr><td>20${course.term}</td><td>${course.subject}</td><td>${course.num}</td><td>${course.sect}</td><td>${course.CRN}</td></tr>
              </tal:blk>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="//oss.maxcdn.com/libs/jquery/1.10.2/jquery.min.js"></script>
    <script src="//oss.maxcdn.com/libs/twitter-bootstrap/3.0.3/js/bootstrap.min.js"></script>
  </body>
</html>
