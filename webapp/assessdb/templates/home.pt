<!DOCTYPE html>
<html lang="${request.locale_name}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width">
    <meta name="description" content="online assessment with easy content editing">
    <meta name="author" content="AssessDB Team">

    <link rel="shortcut icon" href="${request.static_url('assessdb:static/pyramid-16x16.png')}">    
    <link rel="stylesheet" media="screen" href="${request.static_url('assessdb:app/styles/styles.css')}" type="text/css" />
    
    <title>Assessment Database</title>

    <script data-main="app/scripts/main" src="app/scripts/vendor/requirejs/require.js"></script>    
  </head>

  <body>
    <div class="container">
        <div id="header"></div>
        
        <div id="content"></div>
        
        <div id="footer"></div>
    </div>

    <script id="personTemplate" type="text/template">
            <span>Name <%= firstName %>, <%= lastName %> </span>
    </script>

    <script id="headerTemplate" type="text/template">
    <nav class="navbar navbar-default">
